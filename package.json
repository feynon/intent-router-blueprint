{
  "name": "intent-router-blueprint",
  "version": "0.1.0",
  "description": "A dual-LLM intent routing system following the CAMEL security architecture",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src"
  },
  "keywords": ["intent-router", "llm", "camel", "security", "prompt-injection"],
  "author": "Intent Router Blueprint",
  "license": "MIT",
  "dependencies": {
    "@unternet/kernel": "^0.5.0",
    "ollama": "^0.5.16",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "prettier": "^3.5.3",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}